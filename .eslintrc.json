{
    "extends": ["airbnb", "plugin:jest/recommended"],
    "plugins": ["jest"],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "jsx-a11y/label-has-for": [ 2, {
            "required": {
                "some": [ "nesting", "id" ]
            }
        }],
        "jsx-a11y/anchor-is-valid": "off",
        "jsx-quotes": [ "error", "prefer-single" ],
        "quotes": [ "error", "single" ]
    }
}
